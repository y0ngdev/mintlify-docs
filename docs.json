{
  "$schema": "https://mintlify.com/docs.json",
  "background": {
    "color": {
      "dark": "#0c0a09",
      "light": "#fafaf9"
    }
  },
  "colors": {
    "dark": "#18181b",
    "light": "#EAE6E0",
    "primary": "#09090b"
  },
  "servers": [
    {
      "url": "https://api.example.com/v1"
    }
  ],
  "name": "Locket Docs",

  "navigation": {
      "groups": [
          {
            "group": "Introduction",
            "pages": [
              "docs/overview",
              "docs/auth",
              "docs/errors"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": {
              "directory": "docs",
              "source": "https://spec.speakeasy.com/unkey/unkey/openapi-json-with-code-samples"
            }
          }
        ]
  },
  "styling": {
    "codeblocks": "system"
  },
  "theme": "linden",
  "fonts": {
    "family": "Hubot Sans",
    "source": "https://mintlify-assets.b-cdn.net/fonts/Hubot-Sans.woff2",
    "format": "woff2",
    "weight": 400
  }
}
